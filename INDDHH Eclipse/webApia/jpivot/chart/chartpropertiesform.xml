 <tr>
  <td>fmt:chartpropertiesform.chartHeight</td>
  <td>
   <textField type="posint" modelReference="chartHeight" size="5" title="fmt:chartpropertiesform.chartHeightTooltip"/>
   &#160;&#160;Ancho
   <textField type="posint" modelReference="chartWidth" size="5" title="fmt:chartpropertiesform.chartWidthTooltip"/>
  </td>
 </tr>
 
 <tr>
  <td>fmt:chartpropertiesform.background</td>
  <td>
   <textField type="posint" modelReference="bgColorR" size="4" title="fmt:chartpropertiesform.backgRedTooltip"/>
   <textField type="posint" modelReference="bgColorG" size="4" title="fmt:chartpropertiesform.backgGreenTooltip"/>
   <textField type="posint" modelReference="bgColorB" size="4" title="fmt:chartpropertiesform.backgBlueTooltip"/>
  </td>
 </tr>

 <tr>
  <td align="right" colspan="4">
    <button action="validate" label="fmt:chartpropertiesform.okButton" handler="com.tonbeller.wcf.form.ButtonHandler" hide="true"/>
    <button action="revert" label="fmt:chartpropertiesform.cancelButton" handler="com.tonbeller.wcf.form.ButtonHandler" hide="true"/>
  </td>
