<%@page import="com.dogma.vo.*"%><%@page import="com.dogma.vo.custom.DocumentationVo"%><%@include file="../../../components/scripts/server/startInc.jsp" %><HTML><head><%@include file="../../../components/scripts/server/headInclude.jsp" %></head><body><jsp:useBean id="dBean" scope="session" class="com.dogma.bean.administration.DocumentationBean"></jsp:useBean><% DocumentationVo documentationVo = dBean.getDocumentationVo(); %><TABLE class="pageTop"><COL class="col1"><COL class="col2"><TR><TD><%=LabelManager.getName(labelSet,"titDocumen")%></TD><TD></TD></TR></TABLE><div id="divContent"><form id="frmMain" name="frmMain" method="POST"><DIV class="subTit"><%=LabelManager.getName(labelSet,"sbtDocDatos")%></DIV><table class="tblFormLayout"><tr><td title="<%=LabelManager.getToolTip(labelSet,"lblTem")%>"><%=LabelManager.getNameWAccess(labelSet,"lblTem")%>:</td><td colspan=3><select p_required="true" name="txtTemplate" onchange="changeTemplate()" accesskey="<%=LabelManager.getAccessKey(labelSet,"lblTem")%>"><option value=""></option><%	String[][] templates = com.dogma.DogmaConstants.DOC_TEMPLATES;
							boolean isCustomTemplate = documentationVo.getTemplate() != null;
							for (int i=0;i< templates.length;i++) {
								out.print("<option value=\"" + dBean.fmtStr(templates[i][1]) + "\"" );
								if ((documentationVo.getTemplate() != null && 
									documentationVo.getTemplate().equals(templates[i][1])) ||
									documentationVo.getTemplate() == templates[i][1]) {
									out.print(" selected ");
									isCustomTemplate = false;
								} 
								out.print(">");
								out.print(LabelManager.getName(labelSet,templates[i][0]));
								out.print("\n");
							}
							%><option value="<CUSTOM>" <%=isCustomTemplate?"selected":""%>><%=LabelManager.getName(labelSet,"lblTemCustom")%></select>
		   				&nbsp;
		   				<input id=customTemplate type="text" name="txtCusTemplate" size="40" <% if (! isCustomTemplate) { %>disabled="true" "<%=LabelManager.getToolTip(labelSet,"lblTem")%>" style="visibility:hidden"<%} else { %>value="<%=dBean.fmtStr(documentationVo.getTemplate())%>"<% } %>></td></tr><tr><td title="<%=LabelManager.getToolTip(labelSet,"lblTit")%>"><%=LabelManager.getNameWAccess(labelSet,"lblTit")%>:</td><td colspan="3"><input type="text" value="<%=dBean.fmtStr(documentationVo.getTitle())%>" p_required="true" name="txtDocTitle" size=80 accesskey="<%=LabelManager.getNameWAccess(labelSet,"lblTit")%>"></td></tr><tr><td title="<%=LabelManager.getToolTip(labelSet,"lblNom")%>"><%=LabelManager.getNameWAccess(labelSet,"lblNom")%>:</td><td colspan="3"><input type="text" value="<%=dBean.fmtStr(documentationVo.getName())%>" p_required="true" name="txtDocName" size=80 accesskey="<%=LabelManager.getNameWAccess(labelSet,"lblNom")%>"></td></tr><tr><td title="<%=LabelManager.getToolTip(labelSet,"lblUrl")%>"><%=LabelManager.getNameWAccess(labelSet,"lblUrl")%>:</td><td colspan="3"><input type="text"value="<%=dBean.fmtStr(documentationVo.getSite())%>"  p_required="true" name="txtDocSite" size=80 accesskey="<%=LabelManager.getNameWAccess(labelSet,"lblUrl")%>"></td></tr></table><DIV class="subTit"><%=LabelManager.getName(labelSet,"sbtOpc")%></DIV><table class="tblFormLayout"><COL class="col1"><COL class="col2"><COL class="col3"><COL class="col4"><tr><td><input type="checkbox" <% if (documentationVo.isShowEmptyTitle()) {%>checked<%}%> name="chkShowEmptyTitle" value="true" accesskey="<%=LabelManager.getNameWAccess(labelSet,"lblDocShowEmptyTitle")%>"></td><td colspan="3" title="<%=LabelManager.getToolTip(labelSet,"lblDocShowEmptyTitle")%>"><%=LabelManager.getNameWAccess(labelSet,"lblDocShowEmptyTitle")%></td></tr><tr><td><input type="checkbox" <% if (documentationVo.isShowEmptyList()) {%>checked<%}%> name="chkShowEmptyList" value="true" accesskey="<%=LabelManager.getNameWAccess(labelSet,"lblDocShowEmptyList")%>"></td><td colspan="3" title="<%=LabelManager.getToolTip(labelSet,"lblDocShowEmptyList")%>"><%=LabelManager.getNameWAccess(labelSet,"lblDocShowEmptyList")%></td></tr><tr><td><input type="checkbox" <% if (documentationVo.isAddDocuments()) {%>checked<%}%> name="chkAddDocuments" value="true" accesskey="<%=LabelManager.getNameWAccess(labelSet,"lblDocAddDocs")%>"></td><td colspan="3" title="<%=LabelManager.getToolTip(labelSet,"lblDocAddDocs")%>"><%=LabelManager.getNameWAccess(labelSet,"lblDocAddDocs")%></td></tr><tr><td><input type="checkbox" <% if (documentationVo.isAddImages()) {%>checked<%}%> name="chkAddImages" value="true" accesskey="<%=LabelManager.getNameWAccess(labelSet,"lblDocAddImgs")%>"></td><td colspan="3" title="<%=LabelManager.getToolTip(labelSet,"lblDocAddImgs")%>"><%=LabelManager.getNameWAccess(labelSet,"lblDocAddImgs")%></td></tr></table><% if (dBean.isDocumentGenerated()) { %><DIV class="subTit"><%=LabelManager.getName(labelSet,"lblMigDownloadFile")%></DIV><table class="tblFormLayout"><COL class="col1"><COL class="col2"><COL class="col3"><COL class="col4"><tr><td></td><td><button onclick="downloadDocument()" accesskey="<%=LabelManager.getAccessKey(labelSet,"btnDow")%>" title="<%=LabelManager.getToolTip(labelSet,"btnDow")%>"><%=LabelManager.getNameWAccess(labelSet,"btnDow")%></button></td><td></td><td></td></tr></table><IFRAME name="idResult" id="idResult" height="0" width="0" frameborder="0" ></IFRAME><% } %></form></div><TABLE class="pageBottom"><COL class="col1"><COL class="col2"><TR><TD></TD><TD><button type="button" onclick="btnGen_click()" accesskey="<%=LabelManager.getAccessKey(labelSet,"btnGen")%>" title="<%=LabelManager.getToolTip(labelSet,"btnGen")%>"><%=LabelManager.getNameWAccess(labelSet,"btnGen")%></button><button type="button" onclick="btnExit_click()" accesskey="<%=LabelManager.getAccessKey(labelSet,"btnSal")%>" title="<%=LabelManager.getToolTip(labelSet,"btnSal")%>"><%=LabelManager.getNameWAccess(labelSet,"btnSal")%></button></TD></TR></TABLE></body></html><%@include file="../../../components/scripts/server/endInc.jsp" %><script language="javascript" src='<%=Parameters.ROOT_PATH%>/programs/administration/documentation/config.js'></script><script language="javascript">
var TOOL_TIP = "<%=LabelManager.getToolTip(labelSet,"lblTem")%>";
</script><% if (dBean.isDocumentGenerated()) { %><script language="javascript">


	function download(){
	  document.getElementById("idResult").src="<%=Parameters.ROOT_PATH%>/programs/administration/documentation/download.jsp"
	}
	if (document.addEventListener) {
   		window.addEventListener("load", download, false);
	}else{
		window.attachEvent("onload", download);
	}	    	
</script><% } %>